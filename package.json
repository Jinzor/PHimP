{
  "name": "phimp",
  "repository": {
    "type": "git",
    "url": "https://github.com/Jinzor/PHimP.git"
  },
  "scripts": {
    "build": "npm run common && npm run main",
    "common": "babel assets/js/common.js > assets/js/dist/common.js --presets=minify",
    "main": "babel assets/js > assets/js/dist/main.js --ignore assets/js/dist --ignore assets/js/common.js"
  },
  "author": "loc.brisset@gmail.com",
  "dependencies": {
    "js-polyfills": "^0.1.42"
  },
  "babel": {
    "presets": [
      "env"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-minify": "^0.4.3"
  }
}
